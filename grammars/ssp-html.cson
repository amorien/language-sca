'name': 'SCA Server Page (HTML)'
'scopeName': 'text.html.mustache.sca.ssp'
'fileTypes': [
  'ssp'
]
'injections':
  'L:text.html.mustache.sca.ssp - (meta.tag.template.mustache | comment.block | meta.embedded.sca.ssp)':
    'patterns': [
      {
        'include': 'text.html.mustache#mustache'
      }
    ]
'patterns': [
  {
    'begin': '(<%=?)'
    'beginCaptures':
      '1':
        'name': 'entity.name.tag.sca.ssp'
    'end': '[^\\{]?(\\{)|[^\\}]?(\\})|(%>)'
    'endCaptures':
      '1':
        'name': 'meta.brace.curly.js'
      '2':
        'name': 'meta.brace.curly.js'
      '3':
        'name': 'entity.name.tag.sca.ssp'
    'name': 'meta.embedded.sca.ssp'
    'contenName': 'source.js.embedded.sca.ssp'
    'patterns': [
      {
        'include': 'source.js'
      }
    ]
  }
  {
    'begin': '(?<=[^\\{]\\{|[^\\}]\\})'
    'end': '(%>)'
    'endCaptures':
      '1':
        'name': 'entity.name.tag.sca.ssp'
    'name': 'meta.embedded.sca.ssp'
    'contenName': 'source.js.embedded.sca.ssp'
    'patterns': [
      {
        'include': 'source.js'
      }
    ]
  }
  {
    'include': 'text.html.basic'
  }
]
